<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         id="application"
         point="org.eclipse.core.runtime.applications">
      <application>
         <run
               class="com.greenapplesolutions.dbloader.Application">
         </run>
      </application>
   </extension>
   <extension
         point="org.eclipse.ui.perspectives">
      <perspective
            name="Perspective"
            class="com.greenapplesolutions.dbloader.Perspective"
            id="com.greenapplesolutions.dbLoader.perspective">
      </perspective>
   </extension>
   <extension
         point="org.eclipse.ui.views">
      <view
            name="View"
            class="com.greenapplesolutions.dbloader.View"
            id="com.greenapplesolutions.dbLoader.view">
      </view>
   </extension>
   <extension
         point="org.eclipse.ui.perspectiveExtensions">
      <perspectiveExtension
            targetID="*">
         <view
               standalone="true"
               minimized="false"
               relative="org.eclipse.ui.editorss"
               relationship="left"
               id="com.greenapplesolutions.dbLoader.view">
         </view>
      </perspectiveExtension>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            locationURI="menu:org.eclipse.ui.main.menu">
         <menu
               label="File">
            <command
                  commandId="org.eclipse.ui.file.exit"
                  label="Exit">
            </command>
            <command
                  commandId="com.greenapplesolutions.dbLoader.openFileLoader"
                  label="File Loader"
                  mode="FORCE_TEXT"
                  style="push">
            </command>
         </menu>
      </menuContribution>
   </extension>
   <extension
         point="org.eclipse.ui.editors">
      <editor
            class="com.greenapplesolutions.dbloader.editors.FileLoader"
            default="true"
            id="com.greenapplesolutions.dbLoader.fileLoader"
            name="name">
      </editor>
   </extension>
   <extension
         point="org.eclipse.ui.commands">
      <command
            defaultHandler="commands.OpenFileLoaderCommand"
            id="com.greenapplesolutions.dbLoader.openFileLoader"
            name="File Loader">
      </command>
   </extension>

</plugin>
